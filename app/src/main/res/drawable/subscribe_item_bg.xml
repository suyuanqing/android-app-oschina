<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_selected="true">
        <layer-list>
            <item>
                <shape>
                    <stroke android:width="1.0dip" android:color="@color/actionbar_background" android:dashWidth="4.0dip" android:dashGap="2.0dip" />
                    <corners android:radius="20dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_selected="true">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/actionbar_background" />
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/black_dan" />
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/actionbar_background" />
            <solid android:color="@color/subscribe_item_pressed_bg" />
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dip" android:color="@color/black_dan" />
            <corners android:radius="20dp"/>
        </shape>
    </item>
</selector>